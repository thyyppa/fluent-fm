    "description": "A PHP package for FileMaker Server's Data API using a fluent query builder style interface.",
    "type":        "library",
    "license":     "MIT",
    "authors":     [
        {
            "name":  "Travis Hyypp√§",
            "email": "travishyyppa@gmail.com"
        }
    ],
    "require":     {
        "php":                  "^7.2",
        "guzzlehttp/guzzle":    "^7.0.1",
        "ext-json":             "*",
        "ramsey/uuid":          "^3.8 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit":      "8.5.5",
        "symfony/var-dumper":   "^4.1"
    },
    "autoload":    {
        "psr-4": {
            "Hyyppa\\FluentFM\\": "src/"
